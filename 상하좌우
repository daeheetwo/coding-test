Size = int(input())
Move = input().split()

x = 1
y = 1

for i in Move:
    if i == "R":
        if y < Size:
            y = y+1
    elif i == "L":
        if y > 1:
            y = y-1
    elif i == "U":
        if x > 1:
            x = x-1
    elif i == "D":
        if x < Size:
            x = x+1



print(x, y)
